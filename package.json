{
  "name": "validators-pl",
  "description": "A few validators for polish data (mostly id's)",
  "version": "0.1.0",
  "author": {
    "name": "Bartek Polanczyk"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:SzybkiSasza/validators-pl.git"
  },
  "main": "lib/validators",
  "scripts": {
    "build": "babel src -d lib --ignore *.test.js",
    "coverage": "jest --coverage",
    "codacy": "npm run coverage && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage",
    "lint": "eslint --fix src",
    "prepublish": "rm -rf ./lib && npm run build",
    "test": "jest"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "eslint": "^3.14.1",
    "eslint-config-google": "^0.7.1",
    "jest": "^18.1.0"
  },
  "keywords": [
    "validator",
    "passport",
    "address",
    "polish"
  ],
  "dependencies": {
    "codacy-coverage": "^2.0.1",
    "codeclimate-test-reporter": "^0.4.0",
    "lodash": "^4.17.4"
  }
}
